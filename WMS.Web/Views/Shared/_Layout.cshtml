<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="wms, Advantech">
    <meta name="author" content="Advantech Web Team">
    <title>@ViewBag.Title - WMS</title>
     
    <!-- CSS -->
    @Styles.Render("~/content/main")
    @Styles.Render("~/content/addon")
    @Styles.Render("~/content/custom")

    <!-- Js -->
    @Scripts.Render("~/bundles/main")
    @Scripts.Render("~/bundles/addon")

    @RenderSection("styles", false)

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="~/scripts/html5shiv.js"></script>
    <![endif]-->
</head>
<body>
    <div>
        <header>@Html.Partial("_Layout_Header")</header>
    </div>

    <div class="ch-container">
        <div class="row">
            <div class="col-sm-2 col-lg-2">
                <div id="menu">
                    @Html.Partial("_Left_Menu")
                </div>
            </div>

            <div class="col-lg-10 col-sm-10">
                <div>
                    <nav>
                        <div>
                            <ul class="breadcrumb">
                                <li>
                                    <a href="#">Home</a>
                                </li>
                                <li>
                                    <a href="#">Dashboard</a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
                <div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <div class="ch-container">
        <hr />
        <footer>@Html.Partial("_Layout_Footer")</footer>
    </div>

    @Scripts.Render("~/bundles/custom")
    @RenderSection("scripts", false)
</body>
</html>